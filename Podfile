  pod ‘geopackage-ios’, :path => ‘../geopackage-ios’
  pod ‘tiff-ios’, :path => ‘../geopackage-tiff-ios’
  pod ‘wkb-ios’, :path => ‘../geopackage-wkb-ios’
  pod 'AFNetworking', '~> 3.1'
  pod 'Fingertips', '~> 0.4.0'
end
